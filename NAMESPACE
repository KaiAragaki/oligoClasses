import(Biobase)

## PDInfo Classes
exportClasses(PDInfo,
              DBPDInfo,
              SNPPDInfo,
              SNPCNVPDInfo,
              AffyTilingPDInfo,
              AffyExpressionPDInfo,
              AffySNPPDInfo,
              AffySNPCNVPDInfo,
              AffyGeneSTPDInfo,
              ExpressionPDInfo,
              NgsExpressionPDInfo,
